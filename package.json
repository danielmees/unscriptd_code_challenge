{
  "name": "react-coding-challenge",
  "version": "1.0.0",
  "description": "A package containing a simple dummy media API, which should be used in the challenge",
  "scripts": {
    "api": "json-server --port 3010 --watch db.json",
    "start": "node server.js",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles --recursive",
    "tdd": "npm test -- --watch",
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "build": "webpack -p"
  },
  "keywords": [
    "react challenge",
    "tryouts",
    "react developer",
    "hiring"
  ],
  "author": "Unscriptd Ltd",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^4.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "file-loader": "^1.1.11",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.3.0",
    "json-server": "0.10.1",
    "mocha": "^3.5.0",
    "react-addons-test-utils": "^15.1.0",
    "react-flexbox-grid": "^1.1.5",
    "react-hot-loader": "^1.3.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^1.0.1",
    "webpack": "^1.15.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "3.3.6",
    "lodash": "^4.17.4",
    "react": "^15.1.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.1.0"
  }
}
